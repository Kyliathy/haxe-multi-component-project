{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flash",
      "type": "fdb",
      "request": "launch",
      "preLaunchTask": "Flash (debug)",
      "program": "${workspaceFolder}/export/run.swf"
    },
    {
      "name": "JavaScript",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/export/main.js",
      "preLaunchTask": "JS (debug)",
      "sourceMaps": true,
      "outFiles": ["${workspaceFolder}/export/*.js"]
    },
    {
      "name": "Attach to Nodexus",
      "type": "node",
      "request": "attach",
      "restart": true,
      "port": 9229
    },
    {
      "name": "JavaScript Client",
      "type": "chrome",
      "url": "localhost:8080",
      "request": "launch",
      "preLaunchTask": "JS Client (debug)",
      "sourceMaps": true
    },
    {
      "name": "C++",
      "type": "hxcpp",
      "request": "launch",
      "preLaunchTask": "C++ (debug)",
      "program": "${workspaceFolder}/export/cpp/Main-debug.exe"
    },
    {
      "name": "C++ NOBUILD",
      "type": "hxcpp",
      "request": "launch",
      "program": "${workspaceFolder}/export/cpp/Main-debug.exe"
    },
    {
      "name": "HashLink/JIT",
      "type": "hl",
      "request": "launch",
      "preLaunchTask": "HL/JIT (debug)",
      "hxml": "build.hxml",
      "cwd": "${workspaceFolder}",
      "program": "export/run.hl"
    },
    {
      "name": "Interpreter",
      "type": "haxe-eval",
      "request": "launch",
      "args": ["build.hxml", "--interp"]
    },
    {
      "name": "Macro",
      "type": "haxe-eval",
      "request": "launch"
    }
  ]
}
